// Global
.nav-pills,
.mega-menu-block .h4 {
	{% if settings.navigation--font == 'body' %}
		font-family: $bodyFontStack;
		font-weight: $bodyFontWeight;
		font-style: $bodyFontStyle;
	{% else %}
		font-family: $subheadingFontStack;
		font-weight: $subheadingFontWeight;
		font-style: $subheadingFontStyle;
	{% endif %}
	{% if settings.navigation--capitalize %}
		text-transform: uppercase;
		letter-spacing: 0;
		{% else %}
		text-transform: none;
		letter-spacing: 0;
	{% endif %}
	{% if settings.navigation--capitalize and settings.navigation--font == 'body' and settings.typography-body-font-size > 13 %}
		font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
	{% endif %}

	& > li {
    padding: 0 $gutter/2;
    display: flex;
	}
}

.nav .feather {
	&.feather-chevron-down,
	&.feather-chevron-right {
		width: 16px;
		height: 16px;
		min-width: 16px;
	}
}

// Topbar
.nav-bar__top-nav {
	padding: 0;

	@media screen and (min-width: 992px) {
		margin-left: -$gutter/4; // Compensates for the menu item padding
	}

	@media screen and (max-width: 991px) {
		.nav {
			justify-content: center;
		}
	}

	div {
		width: 100%; // This is a fix for IE flex
	}

	.nav-pills > li {
    	padding: $gutter/4;
	}
}

// Main nav
.main-navigation-wrapper {
	.nav {
	  justify-content: center;
	  margin: 0 $gutter/2;
	}

	> ul.nav > li > a,
	.mega-menu--dropdown-wrapper > a {
	  	padding: $gutter/3 0;
    }

    .stuck & {
    	> ul.nav > li > a,
		.mega-menu--dropdown-wrapper > a {
		  	padding: $gutter/5 0;
	    }
    }
}

// Dropdowns
.nav > li.mega-menu {
	position: static;

	.dropdown-menu {
		width: 100%;
		padding: 0;
		text-align: center;
		border-radius: 0;
		border: none;
		border-top: 1px solid $headerBorderColor;
    	margin-top: 0;

		.container {
			display: flex;
			flex-wrap: wrap;
		}

		.mega-menu-block {
			display: flex;
			flex-wrap: wrap;
			width: 100%;
			background: $headerBackground;

			{% if settings.navigation--capitalize and settings.navigation--font == 'body' and settings.typography-body-font-size > 13 %}
			.h4 {
				font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
			}
			{% endif %}
		}

		.image-wrap{
			overflow: hidden;
			width: 100%;
			height: 150px;
			opacity: 0;
			margin-bottom: 10px;
			border: 1px solid $headerBorderColor;

			@media screen and (max-width: 960px) {
				height: 100px;
			}
		}

		.image {
			background-size: cover;
			background-position: center center;
			height: 100%;
			width: 101%;
			-webkit-transition: 0.9s all ease;
			-o-transition: 0.9s all ease;
			transition: 0.9s all ease;
		}

	}

  .mega-menu--image {
		text-align: center;

    img {
      	max-width: 100%;
	    max-height: 50vh;
	    margin: 0 auto;
	    object-fit: contain;
    }

		.h5 {
			text-transform: none;
			font-family: $bodyFontStack;
			font-weight: $bodyFontWeight;
			font-style: $bodyFontStyle;
			font-size: $bodyFontSize;
			letter-spacing: 0;
		}
  }

  .mega-menu--image-title{
    margin-top: $gutter/2;
  }

	.dropdown-submenu {
		display: flex;
		float: none;
		padding: $gutter;
		border-right: 1px solid $headerBorderColor;
		border-bottom: 1px solid $headerBorderColor;

		&.five_columns {
			width: 20%;
		}

		&.four_columns {
			width: 25%;
		}

		&.three_columns {
			width: 33.33%;
		}

		&:last-child {
			border-right: none;
		}

		&:hover {
			.image {
				-webkit-transform: scale(1.1);
				-ms-transform: scale(1.1);
				-o-transform: scale(1.1);
				transform: scale(1.1);
			}
		}

		.inner {
			display: flex;
			flex-direction: column;
			width: 100%;
		}

		.dropdown-submenu__linklist {
			display: block;
			margin: 0;
			padding: 0;

			li {
				list-style: none;
				text-align: left;
				margin: 0;
				text-transform: none;
				font-family: $bodyFontStack;
				font-weight: $bodyFontWeight;
				font-style: $bodyFontStyle;
				font-size: $bodyFontSize;

				a {
					display: block;
					padding: 2px 0;
					text-transform: inherit;
					letter-spacing: 0;
				}
			}
		}
	}
}

.dropdown-envy-toggle {
  display: flex;
  align-items: center;
	svg {
		margin-left: $gutter/6;
	}
}

.mega-menu .dropdown-menu,
.dropdown-submenu,
.dropdown-menu {
  @include easeAnimation(opacity);
}

.dropdown-menu {
	top: 0;
	opacity: 0;
	display: block;
	visibility: hidden;
	margin-top: 0;
  background: $headerBackground;
  border-radius: 0;
	z-index: 3;

	& > li > a {
		&:hover, &:focus{
			opacity: $opacity;
			background: none;
		}
	}

	.open > &,
	.force-open > & {
		visibility: visible;
		opacity: 1;

		.sticky-header-wrapper & {
			visibility: hidden;
			opacity: 0;
		}

		.sticky-header-wrapper.sticky & {
			visibility: visible;
			opacity: 1;
		}
	}

	&.dropdown-menu--standard {
		padding: 0;

		li {
			{% if settings.navigation--capitalize and settings.navigation--font == 'body' and settings.typography-body-font-size > 13 %}
				font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
			{% else %}
				font-size: $bodyFontSize;
			{% endif %}
			padding: $gutter/3 $gutter/2;
			line-height: normal;
		}
	}

	svg {
    margin-left: 5px;
	}
}

.dropdown.mega-menu .dropdown-menu {
	top: 0px;
}

.dropdown-submenu {
	text-align: left;
	position: relative;

	.dropdown-submenu__link {
		display: flex;
		justify-content: space-between;
		align-items: center;
		{% if settings.navigation--capitalize and settings.navigation--font == 'body' and settings.typography-body-font-size > 13 %}
			font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
		{% endif %}
	}

	& > .dropdown-menu {
		top: 0;
		margin-top: -1px;
		left: 100%;
		padding: 0;
	}

	&:hover > .dropdown-menu {
		display: block;
		visibility: visible;
		opacity: 1;
	}

	&.pull-left {
		float: none;

		& > .dropdown-menu {
			left: -100%;
			margin-left: 10px;
		}
	}
}

.dropdown-submenu-left .dropdown-menu {
	left: auto;
	right: 100%;
}

.dropdown.mega-menu.open .dropdown-menu {
  width: 100vw;

  .header-wrapper:not(.stuck) & {
    left: 50%;
    transform: translateX(-50%);
  }
}

