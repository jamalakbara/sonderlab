// Dropdowns (product page and language/currency selectors)

.wetheme-dropdown {
    .wetheme-dropdown__wrapper {
        position: relative;
    }

    button {
        text-align: left;
        border: 1px solid $borderColor;
        padding: 13px 35px 13px 13px; // right has space for caret icon
        position: relative;
        white-space: nowrap;
        background: none;

        span {
            display: block;
            overflow: hidden;
            width: 100%;
        }

        &::after {
            // Caret icon
            content: "";
            border-style: solid;
            border-width: 0 1px 1px 0;
            border-color: $colorPrimary;
            display: inline-block;
            padding: 3px;
            position: absolute;
            -webkit-transform: rotate(45deg);
            transform: rotate(45deg);
            right: 15px;
            top: 18px;
        }
    }

    ul {
        display: flex;
        flex-direction: column;
        text-align: left;
        position: absolute;
        left: 0;
        right: 0;
        top: 100%;
        min-width: 100%;
        list-style: none;
        margin: 0;
        padding: 0;
        color: $colorPrimary;
        background-color: $pageBackground;
        border: 1px solid $borderColor;
        z-index: 3;
        max-height: 50vh;
        overflow-x: auto;
        white-space: nowrap;

        li {
            padding: 10px 10px 12px 10px;
            margin: 0;
            font-family: inherit;
            font-weight: inherit;
            font-weight: inherit;
            cursor: pointer;

            &:hover,
            &.selected {
                background-color: $borderColorSubtle;
            }
        }
    }

    &.wetheme-dropdown--up {
        ul {
            top: auto;
            bottom: 100%;
        }
    }

    &.fit-parent {
        width: 100%;
        button {
            width: 100%;
        }
    }

    &.fit-current {
        display: inline-block;

        ul {
            right: auto;
        }
    }

    &.fit-longest {
        display: inline-block;
    }
}

.wetheme-dropdown-fade-enter-active,
.wetheme-dropdown-fade-leave-active {
    transition: $easeAnimation;
}

.wetheme-dropdown-fade-enter,
.wetheme-dropdown-fade-leave-to {
    opacity: 0;
}

.feather {
