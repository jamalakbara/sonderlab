@media only screen and (max-width: 991px) {

	.collection-sidebar--section {
		position: relative;

		h2 {
			cursor: pointer;
		}

		.chevron-right {
			display: block;
	    position: absolute;
	    top: 0;
	    right: 0;
		}

		&.expanded .chevron-right {
			transform: rotate(-90deg);
		}

		& > *:not(:nth-child(-n+2)) {
			opacity: 0;
			max-height: 0;
			overflow: hidden;
			transition: max-height .8s cubic-bezier(0, 1, 0, 1) -.1s, opacity 400ms ease-in-out;
		}

		img {
			max-width: 300px;
		}

		&.expanded {
			> *:not(:nth-child(-n+2)) {
				max-height: 9999px;
				opacity: 1;
				transition-timing-function: cubic-bezier(0.5, 0, 1, 0);
				transition-delay: 0s;
			}
		}
	}
}

