/*============================================================================
  #General Variables
==============================================================================*/

// Page Background
$pageBackground: {{ settings.bg_color }};

// Font Colors
$colorPrimary: {{ settings.font-color }};
$disabledColorPrimary: {{ settings.font-color | color_modify: 'alpha', 0.5 }};
$linkColor: {{ settings.link-color }};
$activeLinkColor: {{ settings.active-link-color }};

// Button colors
$buttonBackgroundColor: {{ settings.add-to-cart-background-color }};
$buttonBackgroundHoverColor: {{ settings.add-to-cart-background-hover-color }};
$buttonTextColor: {{ settings.add-to-cart-text-color }};

// Badges Colors
$saleColor: {{ settings.sale-label-color }};
$saleTextColor: {{ settings.sale-label-text-color }};
$soldOutColor: {{ settings.sold-out-badge-color }};
$soldOutTextColor: {{ settings.sold-out-badge-text-color }};

// Header Colors
$headerBackground: {{ settings.header-background-color }};
$headerText: {{ settings.header-text-color }};
$headerBorderColor: {{ settings.header-text-color | color_modify: 'alpha', 0.1 }};
$headerHoverBackgroundColor: {{ settings.header-text-color | color_modify: 'alpha', 0.1 }};
$headerOverlayText: {{ settings.header-overlay-text-color }};

$stickyHeaderBackground: {{ settings.sticky-header-background-color }};
$stickyHeaderText: {{ settings.sticky-header-text-color }};
$stickyHeaderBorder: {{ settings.sticky-header-text-color | color_modify: 'alpha', 0.1 }};

// Mobile menu
$mobileMenuText: {{ settings.sidebar-navigation--text-color }};
$mobileMenuBackground: {{ settings.sidebar-navigation--background-color }};
$mobileMenuborderColor: {{ settings.sidebar-navigation--text-color | color_modify: 'alpha', 0.5 }};

// Footer Colors
$footerBackground: {{ settings.footer-background-color }};
$footerText: {{ settings.footer-text-color }};

// Pop up Colors
$popUpText: {{ settings.pop-up-text-color }};
$popUpBackground: {{ settings.pop-up-background-color }};

// Secondary Colors
$secondary-color: rgba(0, 0, 0, 1) !default;
$select-padding : 5px;
$select-radius : 2px !default;

$borderColor: {{ settings.font-color | color_modify: 'alpha', 0.5 }};
$borderColorSubtle: {{ settings.font-color | color_modify: 'alpha', 0.1 }};

// Color schemes
$colorSchemeLightBackground: {{ settings.colorSchemeLightBackground }};
$colorSchemeLightText: {{ settings.colorSchemeLightText }};
$colorSchemeLightInputHover: {{ settings.colorSchemeLightText | color_modify: 'alpha', 0.1 }};

$colorSchemeAccentBackground: {{ settings.colorSchemeFeatureBackground }};
$colorSchemeAccentText: {{ settings.colorSchemeFeatureText }};
$colorSchemeAccentInputHover: {{ settings.colorSchemeFeatureText | color_modify: 'alpha', 0.1 }};

$colorSchemeDarkBackground: {{ settings.colorSchemeDarkBackground }};
$colorSchemeDarkText: {{ settings.colorSchemeDarkText }};
$colorSchemeDarkInputHover: {{ settings.colorSchemeDarkText | color_modify: 'alpha', 0.1 }};


// Opacity
$opacity: 0.6;
$easeAnimation: all 0.2s ease-in-out;

@mixin easeAnimation($property) {
  transition: $property 0.2s ease-in-out;
  -webkit-transition: $property 0.2s ease-in-out;
  -o-transition: $property 0.2s ease-in-out;
}

/*============================================================================
  #Spacing
==============================================================================*/

$gutter: 30px;

// Dropdowns
$dropdown-item-height: 50px !default;
$dropdown-item-height: 50px !default;


/*============================================================================
  #Typography
==============================================================================*/

{% assign header_font = settings.type_header_font %}
{% assign subheading_font = settings.subheading--font %}
{% assign base_font = settings.type_base_font %}

{{ header_font | font_face }}
{{ base_font | font_face }}
{{ subheading_font | font_face }}

{%- assign base_font_bold = base_font | font_modify: 'weight', 'bolder' -%}
{%- assign base_font_italic = base_font | font_modify: 'style', 'italic' -%}
{%- assign base_font_bold_italic = base_font_bold | font_modify: 'style', 'italic' -%}

{% if header_font_bold %}
  {{ header_font_bold | font_face }}
{% endif %}

{% if base_font_bold %}
  {{ base_font_bold | font_face }}
{% endif %}

{% if base_font_italic %}
  {{ base_font_italic | font_face }}
{% endif %}

{% if base_font_bold_italic %}
  {{ base_font_bold_italic | font_face }}
{% endif %}

$font-weight-normal: {{ base_font.weight | default: 400 }};
$font-weight-bold: {{ base_font_bold.weight | default: 700 }};
$font-weight-header-bold: {{ header_font_bold.weight | default: 700 }};

$headerFontStack: {{ header_font.family }}, {{ header_font.fallback_families }};
$headerFontWeight: {{ header_font.weight }};
$headerFontStyle: {{ header_font.style }};
$headerFontTransform: {{ settings.heading-case }};

@mixin header-font-style {
  font-family: $headerFontStack;
  font-weight: $headerFontWeight;
  font-style: $headerFontStyle;
  text-transform: $headerFontTransform
}

// Smaller text is always set to 90% of the base font if needed

$bodyFontStack: {{ base_font.family }}, {{ base_font.fallback_families }};
$bodyFontWeight: {{ base_font.weight }};
$bodyFontStyle: {{ base_font.style }};
$bodyFontSize: {{ settings.typography-body-font-size }}px; // Henceforth known as 1em

@mixin body-font-style {
  font-family: $bodyFontStack;
  font-weight: $bodyFontWeight;
  font-style: $bodyFontStyle;
  font-size: $bodyFontSize;
}

// Subheadings

$subheadingFontStack: {{ subheading_font.family }}, {{ subheading_font.fallback_families }};
$subheadingFontWeight: {{ subheading_font.weight }};
$subheadingFontStyle: {{ subheading_font.style }};
$subheadingFontTransform: {{ settings.subheading--case }};

@mixin subheading-font-style {
  font-family: $subheadingFontStack;
  font-weight: $subheadingFontWeight;
  font-style: $subheadingFontStyle;
  text-transform: $subheadingFontTransform
}

/* Typography variables */

/* Sizing */

{% case settings.heading--base-size %}
  {% when 'small' %}
    $headingBase: 28;
    $subheadingBase: 14;
    $productTitles: 20;
    $subpageHeader: 20;
  {% when 'normal' %}
    $headingBase: 34;
    $subheadingBase: 14;
    $productTitles: 24;
    $subpageHeader: 24;
  {% when 'large' %}
    $headingBase: 40;
    $subheadingBase: 14;
    $productTitles: 28;
    $subpageHeader: 28;
{% endcase %}

// Headings

$headingFontSize: $headingBase + px;
$headingLineHeight: ($headingBase * 1.3) + px;

// Subheadings

$subheadingFontSize: $subheadingBase + px;
$subheadingLineHeight: ($subheadingBase * 1.3) + px;
$subheadingCase: {{ settings.subheading--case }};

$productSecondary: 18px;
$addToCartButtons: 16px;
$addToCartSecondary: 14px;
$siteTitle: 24px;


/*

Automatic z-index management

Example:
z-index: index($z-index, base);

*/

$z-index:
  base,
  sorting-bar,
  modals,
  navigation;

.global-transition {
  transition: all 400ms ease-in-out;
}

@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@-webkit-keyframes fadeInUp {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 50px, 0);
    transform: translate3d(0, 50px, 0);
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@keyframes fadeInUp {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 50px, 0);
    transform: translate3d(0, 50px, 0);
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

.fadeIn {
  -webkit-animation-name: fadeIn;
  animation-name: fadeIn;
  -webkit-transition: -webkit-transform 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s;
  transition: transform 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s;
}

.fadeInUp {
  -webkit-animation-name: fadeInUp;
  animation-name: fadeInUp;
}

.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

.animated.delay-1,
ul.animated-auto-list li:nth-child(1),
.animated-collection-list .indiv-product-wrapper:nth-child(1),
.slider-section--image--slideshow .flex-active-slide .slider-caption .slideshow-fade-content:nth-child(1) {
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}

.animated.delay-2,
ul.animated-auto-list li:nth-child(2),
.animated-collection-list .indiv-product-wrapper:nth-child(2),
.slider-section--image--slideshow .flex-active-slide .slider-caption .slideshow-fade-content:nth-child(2) {
  -webkit-animation-delay: 0.6s;
  animation-delay: 0.6s;
}

.animated.delay-3,
ul.animated-auto-list li:nth-child(3),
.animated-collection-list .indiv-product-wrapper:nth-child(3),
.slider-section--image--slideshow .flex-active-slide .slider-caption .slideshow-fade-content:nth-child(3) {
  -webkit-animation-delay: 0.9s;
  animation-delay: 0.9s;
}

.animated.delay-4,
ul.animated-auto-list li:nth-child(4),
.animated-collection-list .indiv-product-wrapper:nth-child(4),
.slider-section--image--slideshow .flex-active-slide .slider-caption .slideshow-fade-content:nth-child(4) {
  -webkit-animation-delay: 1.2s;
  animation-delay: 1.2s;
}

.animated.delay-5,
ul.animated-auto-list li:nth-child(5),
.animated-collection-list .indiv-product-wrapper:nth-child(5) {
  -webkit-animation-delay: 1.5s;
  animation-delay: 1.5s;
}

.animated.delay-6,
ul.animated-auto-list li:nth-child(6),
.animated-collection-list .indiv-product-wrapper:nth-child(6) {
  -webkit-animation-delay: 1.8s;
  animation-delay: 1.8s;
}

.animated.delay-7,
ul.animated-auto-list li:nth-child(7) {
  -webkit-animation-delay: 2.1s;
  animation-delay: 2.1s;
}

.animated.delay-8,
ul.animated-auto-list li:nth-child(8) {
  -webkit-animation-delay: 2.4s;
  animation-delay: 2.4s;
}

.animated.delay-9,
ul.animated-auto-list li:nth-child(9) {
  -webkit-animation-delay: 2.7s;
  animation-delay: 2.7s;
}

.animated.delay-10,
ul.animated-auto-list li:nth-child(10) {
  -webkit-animation-delay: 3s;
  animation-delay: 3s;
}

.animated.delay-11,
ul.animated-auto-list li:nth-child(11) {
  -webkit-animation-delay: 3.3s;
  animation-delay: 3.3s;
}

.animated.delay-12,
ul.animated-auto-list li:nth-child(12) {
  -webkit-animation-delay: 3.6s;
  animation-delay: 3.6s;
}

@media (print), (prefers-reduced-motion: reduce) {
  .animated {
    -webkit-animation-duration: 1ms !important;
    animation-duration: 1ms !important;
    -webkit-transition-duration: 1ms !important;
    transition-duration: 1ms !important;
    -webkit-animation-iteration-count: 1 !important;
    animation-iteration-count: 1 !important;
  }
}

.animations-disabled{
  .animated,
  .flexslider-homepage li.flex-active-slide .first-fade,
  .flexslider-homepage li.flex-active-slide .second-fade,
  .flexslider-homepage li.flex-active-slide .third-fade{
    -webkit-animation-duration: 1ms !important;
    animation-duration: 1ms !important;
    -webkit-transition-duration: 1ms !important;
    transition-duration: 1ms !important;
    -webkit-animation-iteration-count: 1 !important;
    animation-iteration-count: 1 !important;
    animation-name: none;
    -webkit-animation-delay: 0s;
    animation-delay: 0s;
    transition: none;
  }
}

.flexslider-homepage li .first-fade,
.flexslider-homepage li .second-fade,
.flexslider-homepage li .third-fade{
  opacity: 0;
  -webkit-transform: translateY(100px);
  transform: translateY(100px);
}

.slider-section--image--slideshow li .slider-caption .slideshow-fade-content {
  opacity: 0;
  -webkit-transform: translateY(100px);
  transform: translateY(100px);
}

.slider-section--image--slideshow li.flex-active-slide .slider-caption .slideshow-fade-content:nth-child(1) {
  transition: opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.5s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.4s;
  transition: transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.2s,opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.5s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.2s;
  transform: translateY(0px);
  opacity: 1;
}

.slider-section--image--slideshow li.flex-active-slide .slider-caption .slideshow-fade-content:nth-child(2) {
  transition: opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.6s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.5s;
  transition: transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.5s,opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.6s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.5s;
  transform: translateY(0px);
  opacity: 1;
}

.slider-section--image--slideshow li.flex-active-slide .slider-caption .slideshow-fade-content:nth-child(3) {
  transition: opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.7s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.6s;
  transition: transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.7s,opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.7s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.7s;
  transform: translateY(0px);
  opacity: 1;
}

.slider-section--image--slideshow li.flex-active-slide .slider-caption .slideshow-fade-content:nth-child(4) {
  transition: opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.8s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.7s;
  transition: transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.9s,opacity 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.8s,-webkit-transform 1s cubic-bezier(0.25, 0.75, 0.25, 1) 0.9s;
  transform: translateY(0px);
  opacity: 1;
}

.wow{
  visibility: hidden;
}

.no-js .wow{
	visibility: visible;
}

@media only screen and (max-width:767px){
	.wow {
		visibility: visible !important;
		-webkit-animation: none !important;
		-moz-animation: none !important;
		-o-animation: none !important;
		-ms-animation: none !important;
		animation: none !important;
	}
}

a.button--text-subdued{
	text-decoration: underline;
	display: inline-block;
	margin-top: 10px;
}


input#purchase,
button#purchase,
a#purchase {
	width: 100%;
}

.btn {
	border: 0 none;
	padding: 12px $gutter;
	border-radius: 0;
	background: $buttonBackgroundColor;
	color: $buttonTextColor;
	{% if settings.buttons--font == 'body' %}
		font-family: $bodyFontStack;
		font-weight: $bodyFontWeight;
		font-style: $bodyFontStyle;
		font-size: $bodyFontSize;
	{% else %}
		font-family: $subheadingFontStack;
		font-weight: $subheadingFontWeight;
		font-style: $subheadingFontStyle;
		font-size: $subheadingFontSize;
	{% endif %}
	{% if settings.buttons--capitalize %}
		text-transform: uppercase;
		letter-spacing: 2px;
	{% endif %}
	{% if settings.buttons--capitalize and settings.buttons--font == 'body' and settings.typography-body-font-size > 13 %}
		font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
	{% endif %}
	&:hover {
		background: $buttonBackgroundHoverColor;
		color: $buttonTextColor;
	}
}

.btn-custom{
	border: 0 none;
	margin-bottom: 20px;
	margin-top: 20px;
	padding: 12px $gutter;
	border-radius: 0;
	background: $buttonBackgroundColor;
	color: $buttonTextColor;
	{% if settings.buttons--font == 'body' %}
		font-family: $bodyFontStack;
		font-weight: $bodyFontWeight;
		font-style: $bodyFontStyle;
		font-size: $bodyFontSize;
	{% else %}
		font-family: $subheadingFontStack;
		font-weight: $subheadingFontWeight;
		font-style: $subheadingFontStyle;
		font-size: $subheadingFontSize;
	{% endif %}
	{% if settings.buttons--capitalize %}
		text-transform: uppercase;
		letter-spacing: 2px;
	{% endif %}
	{% if settings.buttons--capitalize and settings.buttons--font == 'body' and settings.typography-body-font-size > 13 %}
		font-size: {{ settings.typography-body-font-size | minus: 2 }}px;
	{% endif %}
}

// Secondary buttons
.secondary-button {
	background: transparent;
	color: $buttonBackgroundColor;
	box-shadow: inset 0 0 0 1px $buttonBackgroundColor;
	transition: $easeAnimation;
	outline: none; // get rid of dotted outline (Firefox and IE)
	{% if settings.buttons--font == 'body' %}
		font-family: $bodyFontStack;
		font-weight: $bodyFontWeight;
		font-style: $bodyFontStyle;
		font-size: $bodyFontSize;
	{% else %}
		font-family: $subheadingFontStack;
		font-weight: $subheadingFontWeight;
		font-style: $subheadingFontStyle;
		font-size: $subheadingFontSize;
	{% endif %}
	{% if settings.buttons--capitalize %}
		text-transform: uppercase;
		letter-spacing: 2px;
	{% endif %}
	&:hover:not([disabled]),
	&:active:not([disabled]),
	&:focus:not([disabled]) {
		box-shadow: inset 0 0 0 2px $buttonBackgroundColor;
		background: transparent;
		color: $buttonBackgroundColor;
	}
	&:hover[disabled],
	&:active[disabled],
	&:focus[disabled] {
		color: $buttonBackgroundColor;
		background: transparent;
		cursor: default;
	}
}

.btn-no-link{
	cursor:default;
}

